{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.18.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-saver": "^2.0.2",
    "marked": "^0.7.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.1.1",
    "react-test-renderer": "^16.9.0",
    "react-textarea-autosize": "^7.1.0",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/Style/_styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/Style/_styles/ --watch --recursive",
    "start-js": "NODE_PATH=src react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "NODE_PATH=src react-scripts build && mv build ../",
    "build": "npm-run-all build-css build-js",
    "godev": "rm -rf build && npm run build",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "test-ci": "NODE_PATH=src CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "prettier": "1.17.1"
  }
}
